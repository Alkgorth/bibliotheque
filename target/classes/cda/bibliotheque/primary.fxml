<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="165.0" prefWidth="192.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cda.bibliotheque.controller.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Button fx:id="primaryButton" onAction="#switchToAuthors" text="Switch to Authors View" />
      <Button fx:id="BooksButton" layoutX="30.0" layoutY="99.0" onAction="#switchToBooks" text="Switch to Books View" />
      <Button fx:id="ClientsButton" layoutX="30.0" layoutY="99.0" onAction="#switchToClients" text="Switch to Client View" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
